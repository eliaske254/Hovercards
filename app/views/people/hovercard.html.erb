<%= turbo_frame_tag dom_id(@person, :personnel) do %>

<p>
  <strong>Address:</strong>
  <%= @person.Address %>
</p>

<p>
  <strong>Phone:</strong>
  <%= @person.Phone %>
</p>

<p>
  <strong>Date of birth:</strong>
  <%= @person.Date_of_birth %>
</p>

<%= link_to "Show this person", @person %>

<% end %>